<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.schelb.fernando.jsoupexemple.MainActivity">

    <TextView
        android:id="@+id/title_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/title_button"
        android:layout_below="@+id/title_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Title"/>

    <TextView
        android:id="@+id/descr_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/title_button"
        android:layout_centerInParent="true"/>

    <Button
        android:id="@+id/descr_button"
        android:layout_below="@+id/descr_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Description"/>

    <ImageView
        android:id="@+id/logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/descr_button"
        android:layout_centerInParent="true"/>

    <Button
        android:id="@+id/logo_button"
        android:layout_below="@+id/logo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Logo"/>

    <TextView
        android:id="@+id/tv_tabela"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/logo_button"
        android:layout_centerInParent="true"/>

    <Button
        android:id="@+id/bt_tabela"
        android:layout_below="@+id/tv_tabela"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tabela"/>


</RelativeLayout>
